import java.util.Scanner;

class HelloWorld {
    public static void main(String[] args) {
        
       int primNumero;
       int segNumero;
       int tercNumero;
       int somaTotal;
        
        
        Scanner scanner = new Scanner(System.in);
        
        System.out.println("Informe 3 numeros de sua escolha:  ");
        primNumero = scanner.nextInt();
        segNumero = scanner.nextInt();
        tercNumero = scanner.nextInt();
    
      
        scanner.close();
        
        somaTotal = primNumero + segNumero + tercNumero;
        
        System.out.println("A soma dos 3 numeros escolhidos e: " + somaTotal);
    }
}
