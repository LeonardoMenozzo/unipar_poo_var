        String nomeSmartphone;
        String nomeLaptop;
        String nomeFoneBluetooth;
        String marcaSmartphone;
        String marcaLaptop;
        String marcaFoneBluetooth;
        double precoSmartphone;
        double precoLaptop;
        double precoFoneBluetooth;
        int quantidadeEstoqueSmartphone;
        int quantidadeEstoqueLaptop;
        int quantidadeEstoqueFoneBluetooth;

     
        nomeSmartphone = "Smartphone Modelo X";
